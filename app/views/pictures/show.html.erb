<h1>Picture Details #<%= @picture.id %></h1>

<%= link_to("Delete this picture", picture_url(@picture.id), :method => "delete") %>

<div>
	<%= image_tag(@picture.source, :width => 480) %>
</div>

<p><%= @picture.caption %></p>

<%= link_to("Go to list of all pictures", pictures_url, :method => "get") %>



